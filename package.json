{
    "name": "theme-pink-candy",
    "displayName": "Pink Candy Theme",
    "description": "A vivid colour scheme with a pink accent, in both light and dark variants.",
    "version": "1.2.0",
    "publisher": "kuba-p",
    "author": {
        "name": "kuba_p"
    },
    "license": "MIT",
    "icon": "icon.png",
    "homepage": "https://github.com/KubaP/vscode-pink-candy/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/KubaP/vscode-pink-candy"
    },
    "bugs": {
        "url": "https://github.com/KubaP/vscode-pink-candy/issues"
    },
    "galleryBanner": {
        "color": "#2F3A43",
        "theme": "dark"
    },
    "markdown": "github",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "theme",
        "highlighting",
        "colour scheme",
        "light",
        "dark"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Pink Candy Light",
                "uiTheme": "vs",
                "path": "./themes/pink-candy-light.json"
            },
            {
                "label": "Pink Candy Dark",
                "uiTheme": "vs-dark",
                "path": "./themes/pink-candy-dark.json"
            }
        ]
    },
    "scripts": {
        "package": "vsce package",
        "deploy": "vsce publish -p",
        "postDeploy": "node -e \"const version = require('./package.json').version; console.log(`##vso[task.setvariable variable=version;isOutput=true]${version}`);\""
    },
    "devDependencies": {
        "vsce": "=2.7.0"
    }
}