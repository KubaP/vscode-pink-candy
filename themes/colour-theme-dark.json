        {
            // The url part of the link.
            "name": "Markup - Url Underline",
            "scope": [
                "markup.underline.link",
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#abb2bf",
                "fontStyle": "underline"
            }
        }
